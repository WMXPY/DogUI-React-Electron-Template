{
  "name": "dogui-react-electron-template",
  "version": "1.0.1",
  "description": ":ballot_box_with_check: DogUI React Electron Template",
  "main": "build/main/main.js",
  "scripts": {
    "pack": "webpack --config webpack.dev.js",
    "build": "webpack --config webpack.config.js --display-modules --sort-modules-by size",
    "dev": "webpack-dev-server --config webpack.dev.js --open",
    "es": "babel src/main --copy-files --presets=es2015 --source-maps --extensions .es6,.es --out-dir build/main",
    "electron": "electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WMXPY/dogui-react-electron-template.git"
  },
  "keywords": [
    "React",
    "DogUI",
    "Dogo",
    "Electron"
  ],
  "author": "WMXPY",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/WMXPY/dogui-react-electron-template/issues"
  },
  "dependencies": {
    "electron": "^1.7.9",
    "electron-json-storage": "^3.1.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "build": {
    "productName": "adogiotemplate",
    "appId": "io.adog.template",
    "files": [
      "build/",
      "node_modules/electron/",
      "package.json",
      "build/renderer/index.html"
    ],
    "mac": {
      "category": "efficiency"
    },
    "win": {}
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "css-loader": "0.28.7",
    "electron-builder": "^19.48.2",
    "html-webpack-plugin": "^2.30.1",
    "mocha": "^4.0.1",
    "react-hot-loader": "next",
    "react-router-dom": "^4.2.2",
    "style-loader": "0.19.0",
    "uglifyjs-webpack-plugin": "v1.1.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.5"
  },
  "homepage": "https://github.com/WMXPY/dogui-react-electron-template#readme"
}